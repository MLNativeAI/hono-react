{
  "name": "hono-react",
  "packageManager": "bun@1.3.4",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test:e2e": "turbo test:e2e",
    "test:e2e:smoke": "turbo test:e2e:smoke",
    "test:e2e:acc": "turbo test:e2e:acc",
    "db:generate": "turbo db:generate",
    "db:migrate": "turbo db:migrate",
    "db:push": "turbo db:push",
    "init": "turbo init"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "turbo": "^2.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "better-auth": "^1.4.13",
      "hono": "^4.9.10",
      "react": "^19.2.3",
      "react-dom": "^19.2.3",
      "drizzle-kit": "^0.31.8",
      "drizzle-orm": "^0.45.1",
      "zod": "^4.0.0"
    }
  },
  "version": "1.7.0"
}
